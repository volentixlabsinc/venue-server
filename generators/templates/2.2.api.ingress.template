apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: "venue-service-ing"
  namespace: venue-{{namespace}}
  labels:
    application: venue
spec:
  rules:
  - host: "service.{{namespace}}.venue.ninja"
    http:
      paths:
      - backend:
          serviceName: "venue-service"
          servicePort: 80